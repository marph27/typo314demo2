lib.containerElement < lib.contentElement
lib.containerElement {
  layoutRootPaths {
    100 = EXT:sitepackage/Resources/Private/Extensions/fluid_styled_content/Resources/Private/Layouts/
    200 = EXT:sitepackage/Resources/Private/Extensions/container/Resources/Private/Layouts/
  }
  partialRootPaths {
    100 = EXT:sitepackage/Resources/Private/Extensions/fluid_styled_content/Resources/Private/Partials/
    200 = EXT:sitepackage/Resources/Private/Extensions/container/Resources/Private/Partials/
  }
  templateRootPaths {
    100 = EXT:sitepackage/Resources/Private/Extensions/fluid_styled_content/Resources/Private/Templates/
    200 = EXT:sitepackage/Resources/Private/Extensions/container/Resources/Private/Templates/
  }

  dataProcessing {
    10 = B13\Container\DataProcessing\ContainerProcessor      
  }
}


#2 Spalten
