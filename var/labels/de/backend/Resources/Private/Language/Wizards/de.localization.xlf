<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file source-language="en" datatype="plaintext" original="EXT:backend/Resources/Private/Language/Wizards/localization.xlf" date="2025-10-28T00:00:00Z" product-name="backend" target-language="de">
    <header/>
    <body>
      <!-- Wizard -->
      <trans-unit id="localization_wizard.modal.title" approved="yes">
        <source>Localization</source>
        <target state="final">Lokalisierung</target>
      </trans-unit>
      <trans-unit id="localization_wizard.modal.title.record_prefix" approved="yes">
        <source>Localize:</source>
        <target state="final">Lokalisieren:</target>
      </trans-unit>
      <trans-unit id="localization_wizard.buttons.cancel" approved="yes">
        <source>Cancel</source>
        <target state="final">Abbrechen</target>
      </trans-unit>
      <trans-unit id="localization_wizard.buttons.next" approved="yes">
        <source>Next</source>
        <target state="final">Nächste</target>
      </trans-unit>
      <trans-unit id="localization_wizard.buttons.previous" approved="yes">
        <source>Previous</source>
        <target state="final">Vorherige</target>
      </trans-unit>
      <trans-unit id="localization_wizard.buttons.finish" approved="yes">
        <source>Finish</source>
        <target state="final">Fertigstellen</target>
      </trans-unit>
      <trans-unit id="localization_wizard.buttons.localize" approved="yes">
        <source>Localize</source>
        <target state="final">Lokalisieren</target>
      </trans-unit>
      <trans-unit id="localization_wizard.error.message" approved="yes">
        <source>Failed to load wizard configuration. Please try again.</source>
        <target state="final">Die Wizard-Konfiguration konnte nicht geladen werden. Bitte lade die Seite neu.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.error.title" approved="yes">
        <source>Configuration error</source>
        <target state="final">Konfigurationsfehler</target>
      </trans-unit>
      <trans-unit id="localization_wizard.loading" approved="yes">
        <source>Loading...</source>
        <target state="final">Wird geladen...</target>
      </trans-unit>
      <trans-unit id="localization_wizard.loading_configuration" approved="yes">
        <source>Loading wizard configuration...</source>
        <target state="final">Konfiguration des Assistenten wird geladen...</target>
      </trans-unit>
      <trans-unit id="localization_wizard.loading_steps" approved="yes">
        <source>Loading step modules...</source>
        <target state="final">Lade Schrittmodule...</target>
      </trans-unit>
      <trans-unit id="localization_wizard.loading_finisher" approved="yes">
        <source>Loading finisher...</source>
        <target state="final">Ladevorgang wird abgeschlossen...</target>
      </trans-unit>
      <trans-unit id="localization_wizard.no_steps_available.message" approved="yes">
        <source>No localization steps are available for this record type.</source>
        <target state="final">Für diesen Datensatztyp sind keine Übersetzungsschritte verfügbar.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.no_steps_available.title" approved="yes">
        <source>Warning</source>
        <target state="final">Warnung</target>
      </trans-unit>
      <trans-unit id="localization_wizard.status.error.message" approved="yes">
        <source>Failed to complete localization:</source>
        <target state="final">Lokalisierung konnte nicht abgeschlossen werden:</target>
      </trans-unit>
      <trans-unit id="localization_wizard.status.error.title" approved="yes">
        <source>Error</source>
        <target state="final">Fehler</target>
      </trans-unit>
      <trans-unit id="localization_wizard.status.pending.message" approved="yes">
        <source>Processing localization...</source>
        <target state="final">Lokalisierung wird verarbeitet...</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.no_data.message" approved="yes">
        <source>No finisher data available. Cannot complete the localization process.</source>
        <target state="final">Keine Finisher-Daten verfügbar. Der Lokalisierungsprozess kann nicht abgeschlossen werden.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.load_error.message" approved="yes">
        <source>Failed to load finisher module:</source>
        <target state="final">Fehler beim Laden des Finisher-Moduls:</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.redirect.success.title" approved="yes">
        <source>Localization completed</source>
        <target state="final">Lokalisierung abgeschlossen</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.redirect.success.description" approved="yes">
        <source>The content has been successfully localized. You will be redirected to the new language version.</source>
        <target state="final">Der Inhalt wurde erfolgreich lokalisiert. Du wirst zur neuen Sprachversion weitergeleitet.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.reload.success.title" approved="yes">
        <source>Localization completed</source>
        <target state="final">Lokalisierung abgeschlossen</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.reload.success.description" approved="yes">
        <source>The content has been successfully localized. The page will reload to show the updated content.</source>
        <target state="final">Der Inhalt wurde erfolgreich lokalisiert. Die Seite wird neu geladen, um den aktualisierten Inhalt anzuzeigen.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.noop.success.title" approved="yes">
        <source>No operation required</source>
        <target state="final">Kein Eingriff erforderlich</target>
      </trans-unit>
      <trans-unit id="localization_wizard.finisher.noop.success.description" approved="yes">
        <source>No records were selected that need processing. The page will reload.</source>
        <target state="final">Es wurden keine Datensätze ausgewählt, die bearbeitet werden müssen. Die Seite wird neu geladen.</target>
      </trans-unit>
      <trans-unit id="localization_wizard.step.error.title" approved="yes">
        <source>Error</source>
        <target state="final">Fehler</target>
      </trans-unit>
      <!-- Step: Confirmation -->
      <trans-unit id="step.confirmation.title" approved="yes">
        <source>Confirmation</source>
        <target state="final">Bestätigung</target>
      </trans-unit>
      <trans-unit id="step.confirmation.headline" approved="yes">
        <source>Confirm localization</source>
        <target state="final">Lokalisierung bestätigen</target>
      </trans-unit>
      <trans-unit id="step.confirmation.description" approved="yes">
        <source>Please review the following localization settings before proceeding:</source>
        <target state="final">Bitte überprüfe die folgenden Lokalisierungseinstellungen, bevor du fortfährst:</target>
      </trans-unit>
      <!-- Step: Content selection -->
      <trans-unit id="step.content_selection.title" approved="yes">
        <source>Content selection</source>
        <target state="final">Auswahl der Inhalte</target>
      </trans-unit>
      <trans-unit id="step.content_selection.headline" approved="yes">
        <source>Select records to localize</source>
        <target state="final">Zu lokalisierende Datensätze auswählen</target>
      </trans-unit>
      <trans-unit id="step.content_selection.description" approved="yes">
        <source>Choose which content elements you would like to translate:</source>
        <target state="final">Wähle die Inhaltselemente aus, die du übersetzen möchtest:</target>
      </trans-unit>
      <trans-unit id="step.content_selection.summary.title" approved="yes">
        <source>Content Element(s)</source>
        <target state="final">Inhaltselement(e)</target>
      </trans-unit>
      <trans-unit id="step.content_selection.summary.amount" approved="yes">
        <source>%s Element(s)</source>
        <target state="final">%s Element(e)</target>
      </trans-unit>
      <trans-unit id="step.content_selection.error.message" approved="yes">
        <source>Failed to load records for selection</source>
        <target state="final">Das Laden der Datensätze für die Auswahl ist fehlgeschlagen</target>
      </trans-unit>
      <trans-unit id="step.content_selection.button.select_all" approved="yes">
        <source>Select all</source>
        <target state="final">Alle auswählen</target>
      </trans-unit>
      <trans-unit id="step.content_selection.button.deselect_all" approved="yes">
        <source>Deselect all</source>
        <target state="final">Alle abwählen</target>
      </trans-unit>
      <trans-unit id="step.content_selection.no_content.message" approved="yes">
        <source>This page does not contain any content elements.</source>
        <target state="final">Diese Seite enthält keine Inhaltselemente.</target>
      </trans-unit>
      <!-- Step: Handler selection -->
      <trans-unit id="step.handler_selection.title" approved="yes">
        <source>Handler</source>
        <target state="final">Handler</target>
      </trans-unit>
      <trans-unit id="step.handler_selection.headline" approved="yes">
        <source>Select localization handler</source>
        <target state="final">Lokalisierungshandler auswählen</target>
      </trans-unit>
      <trans-unit id="step.handler_selection.description" approved="yes">
        <source>Choose how you would like to localize this record:</source>
        <target state="final">Wähle aus, wie du diesen Datensatz lokalisieren möchtest:</target>
      </trans-unit>
      <trans-unit id="step.handler_selection.summary_label" approved="yes">
        <source>Handler</source>
        <target state="final">Handler</target>
      </trans-unit>
      <trans-unit id="step.handler_selection.error" approved="yes">
        <source>Failed to load localization handlers.</source>
        <target state="final">Das Laden der Lokalisierungshandler ist fehlgeschlagen.</target>
      </trans-unit>
      <trans-unit id="step.handler_selection.no_handlers" approved="yes">
        <source>No localization handlers available.</source>
        <target state="final">Keine Lokalisierungshandler verfügbar.</target>
      </trans-unit>
      <!-- Step: Mode -->
      <trans-unit id="step.modes.title" approved="yes">
        <source>Mode</source>
        <target state="final">Modus</target>
      </trans-unit>
      <trans-unit id="step.modes.headline" approved="yes">
        <source>Select localization mode</source>
        <target state="final">Lokalisierungsmodus auswählen</target>
      </trans-unit>
      <trans-unit id="step.modes.description" approved="yes">
        <source>Choose how content should be localized:</source>
        <target state="final">Wähle aus, wie Inhalte lokalisiert werden sollen:</target>
      </trans-unit>
      <trans-unit id="step.modes.summary.title" approved="yes">
        <source>Mode</source>
        <target state="final">Modus</target>
      </trans-unit>
      <trans-unit id="step.modes.error.message" approved="yes">
        <source>Failed to load localization modes.</source>
        <target state="final">Lokalisierungsmodi konnten nicht geladen werden.</target>
      </trans-unit>
      <trans-unit id="step.modes.none_available" approved="yes">
        <source>No localization modes available.</source>
        <target state="final">Keine Lokalisierungsmodi verfügbar.</target>
      </trans-unit>
      <!-- Step: Source language -->
      <trans-unit id="step.source_language.title" approved="yes">
        <source>Source language</source>
        <target state="final">Quellsprache</target>
      </trans-unit>
      <trans-unit id="step.source_language.headline" approved="yes">
        <source>Select source language</source>
        <target state="final">Quellsprache auswählen</target>
      </trans-unit>
      <trans-unit id="step.source_language.description" approved="yes">
        <source>Choose the language from which you would like to translate content:</source>
        <target state="final">Von welcher Sprache möchtest du übersetzen?</target>
      </trans-unit>
      <trans-unit id="step.source_language.summary.title" approved="yes">
        <source>Source language</source>
        <target state="final">Ausgangssprache</target>
      </trans-unit>
      <trans-unit id="step.source_language.error.message" approved="yes">
        <source>Failed to load source languages.</source>
        <target state="final">Fehler beim Laden der Quellsprachen.</target>
      </trans-unit>
      <trans-unit id="step.source_language.none_available" approved="yes">
        <source>No source languages available.</source>
        <target state="final">Keine Ausgangssprachen verfügbar.</target>
      </trans-unit>
      <!-- Step: Finisher -->
      <trans-unit id="step.finisher.title" approved="yes">
        <source>Finish</source>
        <target state="final">Fertigstellen</target>
      </trans-unit>
      <!-- Step: Target language -->
      <trans-unit id="step.target_language.title" approved="yes">
        <source>Target language</source>
        <target state="final">Zielsprache</target>
      </trans-unit>
      <trans-unit id="step.target_language.headline" approved="yes">
        <source>Select target language</source>
        <target state="final">Zielsprache auswählen</target>
      </trans-unit>
      <trans-unit id="step.target_language.description" approved="yes">
        <source>Select the language into which you would like to translate the content:</source>
        <target state="final">Wähle die Sprache aus, in die du den Inhalt übersetzen möchtest:</target>
      </trans-unit>
      <trans-unit id="step.target_language.summary.title" approved="yes">
        <source>Target language</source>
        <target state="final">Zielsprache</target>
      </trans-unit>
      <trans-unit id="step.target_language.error.message" approved="yes">
        <source>Failed to load target languages.</source>
        <target state="final">Das Laden der Zielsprachen ist fehlgeschlagen.</target>
      </trans-unit>
      <trans-unit id="step.target_language.none_available" approved="yes">
        <source>No target languages available.</source>
        <target state="final">Keine Zielsprachen verfügbar.</target>
      </trans-unit>
      <!-- Handlers -->
      <trans-unit id="handler.manual.label" approved="yes">
        <source>Manual Translation</source>
        <target state="final">Manuelle Übersetzung</target>
      </trans-unit>
      <trans-unit id="handler.manual.description" approved="yes">
        <source>Prepares the language version. Edit and translate the content yourself afterwards.</source>
        <target state="final">Erstellt eine neue Sprachversion. Übersetze die Inhalte danach manuell.</target>
      </trans-unit>
      <!-- Error Messages -->
      <trans-unit id="error.recordNotFound" approved="yes">
        <source>Record with uid "%s" of type "%s" does not exist or is not accessible.</source>
        <target state="final">Der Datensatz mit UID „%s" (Typ: „%s") existiert nicht oder Sie haben keine Zugriffsberechtigung.</target>
      </trans-unit>
    </body>
  </file>
